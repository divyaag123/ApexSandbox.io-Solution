public Map<String, String> phonebook(List<Contact> contacts) {
  Map<String,String> phonebook1 = new Map<String, String>();
  for(Contact con: contacts){
      
      if(contact.phone != NULL){   
          if(phonebook1.contains(contact.name)==NULL)
        phonebook1.put(con.name, con.phone);
      }

 }
 return phonebook1;
}